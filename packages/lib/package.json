{
    "name": "@aolyang/eslint-config",
    "version": "0.0.0-beta.3",
    "type": "module",
    "author": "aolyang <aolyang@foxmail.com>",
    "repository": "git@github.com:aolyang/eslint-config.git",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": { "default": "./dist/index.js", "types": "./dist/index.d.ts" },
            "require": { "default": "./dist/index.cjs", "types": "./dist/index.d.cts" }
        },
        "./stylistic": {
            "import": { "default": "./dist/stylistic/index.js", "types": "./dist/stylistic/index.d.ts" },
            "require": { "default": "./dist/stylistic/index.cjs", "types": "./dist/stylistic/index.d.cts" }
        },
        "./react": {
            "import": { "default": "./dist/react/index.js", "types": "./dist/react/index.d.ts" },
            "require": { "default": "./dist/react/index.cjs", "types": "./dist/react/index.d.cts" }
        }
    },
    "scripts": {
        "build": "tsup --format esm,cjs --dts",
        "dev": "pnpm build --watch"
    },
    "dependencies": {
        "@eslint-react/eslint-plugin": "^1.23.2",
        "@stylistic/eslint-plugin": "^2.12.1",
        "@typescript-eslint/eslint-plugin": "^8.19.0",
        "@typescript-eslint/parser": "^8.19.0",
        "astro-eslint-parser": "^1.1.0",
        "eslint": "^9.17.0",
        "eslint-merge-processors": "^0.1.0",
        "eslint-plugin-astro": "^1.3.1",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "eslint-plugin-svelte": "^2.46.1",
        "eslint-plugin-vue": "^9.32.0",
        "eslint-processor-vue-blocks": "^0.1.2",
        "svelte-eslint-parser": "^0.43.0",
        "vue-eslint-parser": "^9.4.3"
    },
    "peerDependencies": {
        "eslint": "^9.17.0"
    }
}
